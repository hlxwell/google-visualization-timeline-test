<h1>New clicks</h1>

<% form_for(@clicks) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :duration %><br />
    <%= f.datetime_select :duration %>
  </p>
  <p>
    <%= f.label :type_id %><br />
    <%= f.text_field :type_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', clicks_path %>